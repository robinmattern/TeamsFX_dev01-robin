{
  "type": "Container",
  "selectAction": {
    "type": "Action.OpenUrl",
    "title": "${Name}",
    "url": "${WebLink}"
  },
  "items": [
    {
      "type": "Container",
      "items": [
        {
          "type": "Container",
          "items": [
            {
              "type": "ColumnSet",
              "columns": [
                {
                  "type": "Column",
                  "width": "auto",
                  "items": [
                    {
                      "type": "Image",
                      "url": "data:image/png;base64,",
                      "horizontalAlignment": "Left",
                      "spacing": "None"
                    }
                  ]
                },
                {
                  "type": "Column",
                  "width": "stretch",
                  "items": [
                    {
                      "type": "TextBlock",
                      "text": "${Subject}",
                      "height": "stretch",
                      "size": "Large",
                      "weight": "Bolder",
                      "color": "Accent",
                      "fontType": "Default",
                      "spacing": "None"
                    }
                  ],
                  "horizontalAlignment": "Left",
                  "spacing": "Small"
                }
              ]
            }
          ],
          "spacing": "Small"
        }
      ]
    },
    {
      "type": "TextBlock",
      "text": "**From:** ${From}",
      "wrap": true,
      "spacing": "None"
    },
    {
      "type": "TextBlock",
      "text": "**Created:** ${CreatedDatedTime}",
      "spacing": "None"
    },
    {
      "type": "TextBlock",
      "wrap": true,
      "text": "**Has attachments:** ${HasAttachments}",
      "spacing": "None"
    }
  ]
}
