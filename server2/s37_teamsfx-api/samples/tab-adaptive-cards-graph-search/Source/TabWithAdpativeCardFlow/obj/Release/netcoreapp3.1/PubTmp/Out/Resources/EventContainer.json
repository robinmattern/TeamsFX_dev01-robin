{
  "type": "Container",
  "items": [
    {
      "type": "Container",
      "items": [
        {
          "type": "ColumnSet",
          "columns": [
            {
              "type": "Column",
              "width": "auto",
              "items": [
                {
                  "type": "Image",
                  "url": "data:image/png;base64,",
                  "horizontalAlignment": "Left",
                  "spacing": "None",
                  "style": "Person"
                }
              ]
            },
            {
              "type": "Column",
              "width": "stretch",
              "items": [
                {
                  "type": "TextBlock",
                  "text": "${Subject}",
                  "height": "stretch",
                  "size": "Large",
                  "weight": "Bolder",
                  "color": "Accent",
                  "fontType": "Default",
                  "spacing": "None"
                }
              ],
              "horizontalAlignment": "Left",
              "spacing": "Small"
            }
          ]
        }
      ]
    },
    {
      "type": "TextBlock",
      "text": "**Start:** ${Start}",
      "wrap": true,
      "spacing": "None"
    },
    {
      "type": "TextBlock",
      "text": "**End:** ${End}",
      "wrap": true,
      "spacing": "None"
    },
    {
      "type": "TextBlock",
      "wrap": true,
      "text": "**Is all day event:** ${IsAllDay}",
      "spacing": "None"
    }
  ]
}
